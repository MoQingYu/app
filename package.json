{
  "name": "learn-webpack",
  "version": "1.0.0",
  "description": "learn webpack",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development BASE_ENV=development webpack serve -c build/dev.ts",
    "build": "cross-env NODE_ENV=production BASE_ENV=development webpack -c build/prod.ts",
    "build:test": "cross-env NODE_ENV=production BASE_ENV=test webpack -c build/prod.ts",
    "build:pre": "cross-env NODE_ENV=production BASE_ENV=pre webpack -c build/prod.ts",
    "build:prod": "cross-env NODE_ENV=production BASE_ENV=production webpack -c build/prod.ts",
    "build:analy": "cross-env NODE_ENV=production BASE_ENV=production webpack -c build/analy.ts",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,jsx,ts,tsx,json,css,scss,html,md}\"",
    "lint:eslint": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src"
  },
  "dependencies": {
    "antd": "^5.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
