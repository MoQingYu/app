    "open": "^9.1.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "postcss-loader": "^7.3.0",
    "prettier": "^2.8.8",
    "purgecss-webpack-plugin": "^5.0.0",
    "react-refresh": "^0.14.0",
    "sass-loader": "^13.2.2",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.2",
